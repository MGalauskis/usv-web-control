        "symbol-placement": "line",
        "symbol-spacing": 350,
        "text-field": "{ref}",
        "text-font": ["Metropolis Light", "Noto Sans Regular"],
        "text-pitch-alignment": "viewport",
        "text-rotation-alignment": "viewport",
        "text-size": 10,
        "visibility": "visible"
      },
      "paint": {
        "text-color": "#4ecca3",
        "text-halo-blur": 1,
        "text-halo-color": "#0d1b2a",
        "text-halo-width": 1,
        "text-translate": [0, 2]
