    },
    {
      "id": "highway_name_motorway",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "transportation_name",
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["==", "class", "motorway"]
